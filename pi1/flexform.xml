<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
		<!-- <langChildren>1</langChildren>!-->
	</meta>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:ke_contacts/pi1/locallang.xml:tt_content.pi_flexform.sheet_general</sheetTitle>
				</TCEforms>
				<el>
				<storage_pid>
					<TCEforms>
						<exclude>1</exclude>
						<label>LLL:EXT:ke_contacts/pi1/locallang.xml:tt_content.pi_flexform.storage_pid</label>
						<config>
							<type>group</type>
							<internal_type>db</internal_type>
							<allowed>pages</allowed>
							<size>5</size>
							<maxitems>99</maxitems>
							<minitems>1</minitems>
							<show_thumbs>1</show_thumbs>
						</config>
					</TCEforms>
				</storage_pid>
				<css_template_file>
					<TCEforms>
						<label>LLL:EXT:ke_contacts/pi1/locallang.xml:tt_content.pi_flexform.css_template_file</label> 
						<config>
							<type>group</type> 
							<internal_type>file</internal_type> 
							<allowed>css</allowed> 
							<max_size>500</max_size> 
							<uploadfolder>uploads/</uploadfolder> 
							<maxitems>1</maxitems> 
							<size>1</size> 
						</config>
					</TCEforms>
				</css_template_file>
				</el>
			</ROOT>
		</sDEF>
		<tDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:ke_contacts/pi1/locallang.xml:tt_content.pi_flexform.sheet_list</sheetTitle>
				</TCEforms>
				<el>
				<contacts_per_page>
					<TCEforms>
						<exclude>1</exclude>
						<label>
							LLL:EXT:ke_contacts/pi1/locallang.xml:tt_content.pi_flexform.contacts_per_page
						</label>
						<config>
							<type>input</type>
							<default>25</default>
							<size>2</size>
							<max>2</max>
							<eval>int</eval>
						</config>
					</TCEforms>
				</contacts_per_page>
				<company_row_color>
					<TCEforms>
						<exclude>1</exclude>
						<label>
							LLL:EXT:ke_contacts/pi1/locallang.xml:tt_content.pi_flexform.company_row_color
						</label>
						<config>
							<type>input</type>
							<size>10</size>
							<wizards>
								<colorpick>
									<type>colorbox</type>
									<title>LLL:EXT:partner/locallang.php:tx_partner.label.color_picker</title>
									<script>wizard_colorpicker.php</script>
									<dim>20x20</dim>
									<tableStyle>border: solid 1px black; margin-left: 20px;</tableStyle>
									<JSopenParams>height=550,width=365,status=0,menubar=0,scrollbars=1</JSopenParams>
									<exampleImg>gfx/wizard_colorpickerex.jpg</exampleImg>
								</colorpick>
							</wizards>
						</config>
					</TCEforms>
				</company_row_color>
				<list_template_file>
					<TCEforms>
						<label>LLL:EXT:ke_contacts/pi1/locallang.xml:tt_content.pi_flexform.list_template_file</label> 
						<config>
							<type>group</type> 
							<internal_type>file</internal_type> 
							<allowed>htm,html</allowed> 
							<max_size>500</max_size> 
							<uploadfolder>uploads/</uploadfolder> 
							<maxitems>1</maxitems> 
							<size>1</size> 
						</config>
					</TCEforms>
				</list_template_file>
				</el>
			</ROOT>
		</tDEF>
		<wDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:ke_contacts/pi1/locallang.xml:tt_content.pi_flexform.sheet_single</sheetTitle>
				</TCEforms>
				<el>
				<show_comments>
					<TCEforms>
						<exclude>1</exclude>
						<label>
							LLL:EXT:ke_contacts/pi1/locallang.xml:tt_content.pi_flexform.show_comments
						</label>
						<config>
							<type>check</type>
							<default>1</default>
						</config>
					</TCEforms>
				</show_comments>
				<single_template_file>
					<TCEforms>
						<label>LLL:EXT:ke_contacts/pi1/locallang.xml:tt_content.pi_flexform.single_template_file</label> 
						<config>
							<type>group</type> 
							<internal_type>file</internal_type> 
							<allowed>htm,html</allowed> 
							<max_size>500</max_size> 
							<uploadfolder>uploads/</uploadfolder> 
							<maxitems>1</maxitems> 
							<size>1</size> 
						</config>
					</TCEforms>
				</single_template_file>
				</el>
			</ROOT>
		</wDEF>
		<uDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:ke_contacts/pi1/locallang.xml:tt_content.pi_flexform.sheet_edit</sheetTitle>
				</TCEforms>
				<el>
				<edit_template_file>
					<TCEforms>
						<label>LLL:EXT:ke_contacts/pi1/locallang.xml:tt_content.pi_flexform.edit_template_file</label> 
						<config>
							<type>group</type> 
							<internal_type>file</internal_type> 
							<allowed>htm,html</allowed> 
							<max_size>500</max_size> 
							<uploadfolder>uploads/</uploadfolder> 
							<maxitems>1</maxitems> 
							<size>1</size> 
						</config>
					</TCEforms>
				</edit_template_file>
				</el>
			</ROOT>
		</uDEF>
		<vDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:ke_contacts/pi1/locallang.xml:tt_content.pi_flexform.sheet_delete</sheetTitle>
				</TCEforms>
				<el>
				<delete_template_file>
					<TCEforms>
						<label>LLL:EXT:ke_contacts/pi1/locallang.xml:tt_content.pi_flexform.delete_template_file</label> 
						<config>
							<type>group</type> 
							<internal_type>file</internal_type> 
							<allowed>htm,html</allowed> 
							<max_size>500</max_size> 
							<uploadfolder>uploads/</uploadfolder> 
							<maxitems>1</maxitems> 
							<size>1</size> 
						</config>
					</TCEforms>
				</delete_template_file>
				</el>
			</ROOT>
		</vDEF>
	</sheets>
</T3DataStructure>